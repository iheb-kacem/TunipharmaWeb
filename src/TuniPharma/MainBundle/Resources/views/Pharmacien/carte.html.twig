
<div id='map-canvas'></div>
<div class="page-header">

</div>

<div class='col-sm-12'>
    <form class="form form-horizontal" action="" method="POST" >
        
        <div class="form-group">
            <label class="col-md-3 control-label" for="input-text">Latitude</label>
            <div class="col-md-5">
                <input id="input-lat" class="form-control" type="text" placeholder="Text field" value="{{ pharmacie.lat }}" disabled="">
            </div>
        </div>
        <div class="form-group">
            <label class="col-md-3 control-label" for="input-text">Longitude</label>
            <div class="col-md-5">
                <input id="input-log" class="form-control" type="text" placeholder="Text field" value="{{ pharmacie.log }}" disabled="">
            </div>
        </div>
        <div class='col-sm-7 col-sm-offset-1'>
            <div class='col-md-9 col-md-offset-3'>
                <button class='btn btn-contrast' id='submit_location' type='button'>
                    <i class='icon-save'></i>
                    Enregistrer
                </button>
            </div>
        </div>
    </form>
</div>
